{
	"name": "kjkent.dev",
	"private": true,
	"type": "module",
	"version": "0.0.1",
	"packageManager": "pnpm@9.13.2",
	"scripts": {
		"build": "rm -rf ./dist; pnpm run check && astro build",
		"deploy": "pnpm run build && wrangler pages deploy ./dist; rm -r ./dist",
		"dev": "pnpm run build && wrangler pages dev ./dist; rm -r ./dist",
		"lint": "biome check --write",
		"up": "pnpm dlx @astrojs/upgrade && pnpm up -Lr && biome migrate --write"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/react": "^4.1.0",
		"@astrojs/tailwind": "^5.1.3",
		"@biomejs/biome": "1.9.4",
		"@iconify-json/mdi": "^1.2.1",
		"@iconify/tailwind": "^1.2.0",
		"astro": "5.0.5",
		"astro-global": "^2.0.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"tailwindcss": "^3.4.16"
	},
	"devDependencies": {
		"@astrojs/ts-plugin": "^1.10.4",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.2",
		"typescript": "^5.7.2",
		"wrangler": "^3.95.0"
	}
}
